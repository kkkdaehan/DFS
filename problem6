# 길이가 91인 리스트생성한후 0으로 초기화
d = [0] * 91

def fibo(x):
    # 기저 조건: 0번째 피보나치는 0, 1번째 피보나치는 1
    if x == 0:
        return 0
    if x == 1:
        return 1

    # 이미 계산한 값이 있으면 바로 반환
    if d[x] != 0:
        return d[x]

    # 아직 계산하지 않았다면 점화식에 따라 계산 후 저장
    d[x] = fibo(x - 1) + fibo(x - 2)
    return d[x]

def main():
    n = int(input("n번째 피보나치 수를 구할 n을 입력하세요 (0 ≤ n ≤ 90): "))
    print(f"{n}번째 피보나치 수: {fibo(n)}")

if __name__ == "__main__":
    main()
