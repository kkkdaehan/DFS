d = [0] * 91

def fibo(x):
    if x == 0:
        return 0
    if x == 1:
        return 1

    if d[x] != 0:
        return d[x]

    d[x] = fibo(x - 1) + fibo(x - 2)
    return d[x]

def main():
    n = int(input("n번째 피보나치 수를 구할 n을 입력하세요 (0 ≤ n ≤ 90): "))
    print(f"{n}번째 피보나치 수: {fibo(n)}")

if __name__ == "__main__":
    main()
