N = int(input().strip())

move = input().split()
x, y = 1, 1

dy = {'L': 0,  'R': 0,  'U': -1, 'D': 1}
dx = {'L': -1, 'R': 1,  'U': 0,  'D': 0}



for p in move:
    
    nx = x + dy[p]
    ny = y + dx[p]

    #격자 범위를 벗어나변 무시!
    if not (1 <= nx <= N and 1 <= ny <= N):
        continue

    
    x, y = nx, ny

# 결과
print(x, y)
